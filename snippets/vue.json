{
  "nuxt-2-ts": {
    "prefix": [
      "n2t"
    ],
    "body": [
      "<script lang=\"ts\">",
      "import Vue from 'vue'",
      "",
      "export default Vue.extend({",
      "\tname: '$1',",
      "})",
      "</script>",
      "",
      "<template>",
      "\t<div :class=\"\\$style.container\"></div>",
      "</template>",
      "",
      "<style lang=\"scss\" module>",
      ".container {",
      "\twidth: 100%;",
      "}",
      "</style>"
    ]
  },
  "setup": {
    "prefix": [
      "setup",
      "template",
      "vst"
    ],
    "body": [
      "<script lang=\"ts\" setup>",
      "",
      "</script>",
      "",
      "<template>",
      "\t<div>page</div>",
      "</template>",
      "",
      "<style lang=\"scss\" scoped>",
      "",
      "</style>"
    ],
    "description": "My standard setup Vue3 + TS"
  },
  "vue-class-name": {
    "prefix": [
      "vcn"
    ],
    "body": [
      "div[:class=\"cls.b('$1')\"]>$2{}"
    ],
    "description": "My standard setup Vue3 + TS"
  },
  "vue-props-data": {
    "prefix": [
      "vpd"
    ],
    "body": [
      "<script lang=\"ts\" setup>",
      "interface Props {",
      "  value: string",
      "}",
      "",
      "const props = defineProps<Props>()",
      "</script>",
      "",
      "<template>",
      "\t<div>",
      "    {{ props.value }}",
      "\t</div>",
      "</template>",
      "",
      "<style lang=\"scss\" scoped>",
      "",
      "</style>"
    ],
    "description": "My standard setup Vue3 + TS"
  }
}
